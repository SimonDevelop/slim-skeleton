{
  "name": "simondevelop/slim-skeleton",
  "description": "Simple skeleton slim 3 with twig and middlewares tokens",
  "keywords": [
    "template",
    "slim",
    "twig"
  ],
  "homepage": "https://github.com/SimonDevelop/slim-skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "SimonDevelop",
      "email": "contact@simon-micheneau.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "require": {
    "php": ">=7.0",
    "slim/slim": "^3.9",
    "slim/twig-view": "^2.2",
    "slim/csrf": "^0.8.2"
  },
  "require-dev": {
      "phpunit/phpunit": ">=4.8 < 6.0"
  },
  "config": {
        "process-timeout" : 0
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public index.php",
    "test": "phpunit --colors"
  }
}
